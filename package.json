{
  "name": "virtual-poster",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "init_data": "ts-node -O '{\"module\": \"commonjs\"}' src/server/initialize_data.ts",
    "test": "jest --runInBand --forceExit",
    "prod": "NODE_ENV=production ts-node -O '{\"module\": \"commonjs\"}' src/server/index.ts | pino-pretty",
    "prod:log": "NODE_ENV=production DEBUG_LOG=1 ts-node -O '{\"module\": \"commonjs\"}' src/server/index.ts | pino-pretty",
    "nohup": "NODE_ENV=production nohup node_modules/.bin/ts-node -O '{\"module\": \"commonjs\"}' src/server/index.ts &",
    "socket": "nodemon --exec ts-node -O '{\"module\": \"commonjs\"}' src/server/socket_standalone.ts | bunyan -o short",
    "socket:nohup": "NODE_ENV=production nohup ts-node -O '{\"module\": \"commonjs\"}' src/server/socket_standalone.ts > socket.nohup.out &",
    "serve": "vue-cli-service serve",
    "server": "nodemon --exec ts-node -O '{\"module\": \"commonjs\"}' src/server/index.ts | pino-pretty",
    "dev": "set NODE_ENV=development && nodemon -L >&2 | npm run serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "db": "psql postgres -d virtual_poster"
  },
  "husky": {
    "hooks": {}
  },
